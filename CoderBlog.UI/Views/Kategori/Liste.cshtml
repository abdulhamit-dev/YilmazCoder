@using CoderBlog.Entities
@model List<Kategori>
@{
    ViewData["Title"] = "Liste";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
@foreach (var item in Model)
{

    <div class="col-md-3" style="margin-top:10px">
        <div class="card">
            <div class="card-body">
                @item.Adi               
            </div>
        </div>
    </div>
}

</div>

